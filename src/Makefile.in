SHELL = @SHELL@
@SUBMAKE@


top_srcdir = @top_srcdir@
srcdir = @srcdir@
top_builddir = ..
subdir = src
conf_auxdir = @conf_auxdir@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
sbindir = @sbindir@
libdir = @libdir@
linuxdir = @linuxdir@
mandir = @mandir@
datadir = @datadir@/click

CLICK_BUILDTOOL = @CLICK_BUILDTOOL@
CLICK_COMPILE = @CLICK_COMPILE@

VPATH = .:$(top_srcdir)/$(subdir):$(top_srcdir)

CC = @CC@
CPP = @CPP@
CXX = @CXX@
CXXCPP = @CXXCPP@
LD = @LD@
STRIP = @STRIP@
AR = @AR@
RANLIB = @RANLIB@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL@ -m 644
mkinstalldirs = $(conf_auxdir)/mkinstalldirs

.SUFFIXES:
.SUFFIXES: .c .cc .o .ii

.c.o:
	$(COMPILE) -c $< -o $@
.cc.o:
	$(CXXCOMPILE) -c $< -o $@
.cc.ii:
	$(CXXCOMPILE) -E $< > $@


OBJS = fromdevice.o fromdump.o fromipsumdump.o fromnetflowsumdump.o \
	toipsumdump.o todump.o \
	anonipaddr.o classifier.o discard.o fakepcap.o ipfilter.o mergebyts.o \
	pokehandlers.o progressbar.o randomsample.o tee.o \
	ipsumdump.o elements.o

CPPFLAGS = @CPPFLAGS@ -g -MD -DCLICK_USERLEVEL
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@ @CLICKLIB@ `$(CLICK_COMPILE) --libs`
DEFS = @DEFS@ `$(CLICK_COMPILE) --defs`
INCLUDES = -I$(top_builddir) -I$(srcdir) @CLICKINCLUDES@

CXXCOMPILE = $(CXX) $(INCLUDES) $(DEFS) $(CPPFLAGS) $(CXXFLAGS)
CXXLD = $(CXX)
CXXLINK = $(CXXLD) $(CXXFLAGS) $(LDFLAGS) -o $@
COMPILE = $(CC) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)

all: ipsumdump
all-local: ipsumdump

ipsumdump: $(OBJS) @CLICKLIBFILE@
	$(CXXLINK) -rdynamic $(OBJS) $(LIBS)

Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
	cd $(top_builddir) && \
	  CONFIG_FILES=$(subdir)/$@ CONFIG_HEADERS= $(SHELL) ./config.status

elemlist: always
	@rm -f elements.conf
	@$(MAKE) elements.conf
elements.conf: $(CLICK_BUILDTOOL)
	echo $(srcdir) | $(CLICK_BUILDTOOL) findelem -S -r userlevel > elements.conf
elements.cc: elements.conf $(CLICK_BUILDTOOL)
	$(CLICK_BUILDTOOL) elem2export -S < elements.conf > elements.cc
	@rm -f elements.d

install: ipsumdump
	$(mkinstalldirs) $(bindir)
	$(INSTALL) ipsumdump $(bindir)/ipsumdump

always:
	@:

#!gmake
DEPFILES := $(wildcard *.d)
ifneq ($(DEPFILES),)
include $(DEPFILES)
endif
#!end gmake

clean:
	-rm -f *.d *.o elements.mk elements.cc elements.conf ipsumdump
distclean: clean
	-rm -f Makefile

.PHONY: all always clean distclean elemlist
