SHELL = @SHELL@
@SUBMAKE@


top_srcdir = @top_srcdir@
srcdir = @srcdir@
top_builddir = ..
subdir = src
conf_auxdir = @conf_auxdir@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
sbindir = @sbindir@
libdir = @libdir@
linuxdir = @linuxdir@
mandir = @mandir@
datadir = @datadir@/click

CLICK_BUILDTOOL = @CLICK_BUILDTOOL@
CLICK_COMPILE = @CLICK_COMPILE@

VPATH = .:$(top_srcdir)/$(subdir):$(top_srcdir)

CC = @CC@
CPP = @CPP@
CXX = @CXX@
CXXCPP = @CXXCPP@
LD = @LD@
STRIP = @STRIP@
AR = @AR@
RANLIB = @RANLIB@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL@ -m 644
mkinstalldirs = $(conf_auxdir)/mkinstalldirs

.SUFFIXES:
.SUFFIXES: .c .cc .o .ii

.c.o:
	$(COMPILE) -c $< -o $@
.cc.o:
	$(CXXCOMPILE) -c $< -o $@
.cc.ii:
	$(CXXCOMPILE) -E $< > $@


IPSUMDUMP_ELEMENT_OBJS = \
	fromdagdump.o fromdevice.o fromdump.o fromfile.o fromipsumdump.o \
	fromnetflowsumdump.o fromnlanrdump.o fromtcpdump.o \
	ipsumdumpinfo.o ipsumdump_anno.o ipsumdump_ip.o ipsumdump_tcp.o \
	toipsumdump.o todump.o \
	anonipaddr.o changeuid.o classifier.o counter.o drivermanager.o \
	discard.o fakepcap.o ipfilter.o ipnameinfo.o progressbar.o \
	randomsample.o script.o switch.o tee.o timefilter.o timesortedsched.o \
	unqueue.o

IPSUMDUMP_OBJS = \
	$(IPSUMDUMP_ELEMENT_OBJS) ipsumdump.o sd_elements.o

IPAGGCREATE_ELEMENT_OBJS = \
	fromdagdump.o fromdump.o fromfile.o fromipsumdump.o \
	fromnetflowsumdump.o fromnlanrdump.o fromtcpdump.o \
	ipsumdumpinfo.o \
	aggcounter.o aggregateip.o aggregateipflows.o aggregatelen.o \
	anonipaddr.o classifier.o counter.o discard.o drivermanager.o \
	fakepcap.o ipfieldinfo.o ipfilter.o ipmirror.o ipnameinfo.o \
	progressbar.o randomsample.o script.o tee.o timefilter.o timerange.o \
	todump.o

IPAGGCREATE_OBJS = \
	$(IPAGGCREATE_ELEMENT_OBJS) ipaggcreate.o ac_elements.o


CPPFLAGS = @CPPFLAGS@ -g -MD -DCLICK_USERLEVEL
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@
DEPCFLAGS = @DEPCFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@ @CLICKLIB@ `$(CLICK_COMPILE) --otherlibs`
DEFS = @DEFS@ `$(CLICK_COMPILE) --defs`
INCLUDES = -I$(top_builddir) -I$(srcdir) @CLICKINCLUDES@

CXXCOMPILE = $(CXX) $(INCLUDES) $(DEFS) $(CPPFLAGS) $(CXXFLAGS) $(DEPCFLAGS)
CXXLD = $(CXX)
CXXLINK = $(CXXLD) $(CXXFLAGS) $(LDFLAGS) -o $@
COMPILE = $(CC) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS) $(DEPCFLAGS)

all: ipsumdump ipaggcreate
all-local: ipsumdump ipaggcreate

ipsumdump: $(IPSUMDUMP_OBJS) @CLICKLIBFILE@
	$(CXXLINK) -rdynamic $(IPSUMDUMP_OBJS) $(LIBS)

ipaggcreate: $(IPAGGCREATE_OBJS) @CLICKLIBFILE@
	$(CXXLINK) -rdynamic $(IPAGGCREATE_OBJS) $(LIBS)

Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
	cd $(top_builddir) && \
	  CONFIG_FILES=$(subdir)/$@ CONFIG_HEADERS= $(SHELL) ./config.status

elemlist: always
	@rm -f sd_elements.conf ac_elements.conf
	@$(MAKE) sd_elements.conf ac_elements.conf
sd_elements.conf: $(CLICK_BUILDTOOL)
	(for i in $(IPSUMDUMP_ELEMENT_OBJS); do echo "$(srcdir)/$$i" | sed 's/\.o/\.cc/'; done) | $(CLICK_BUILDTOOL) findelem -r userlevel -S > sd_elements.conf
sd_elements.cc: sd_elements.conf $(CLICK_BUILDTOOL)
	$(CLICK_BUILDTOOL) elem2export -S < sd_elements.conf > sd_elements.cc
	@rm -f sd_elements.d
ac_elements.conf: $(CLICK_BUILDTOOL)
	(for i in $(IPAGGCREATE_ELEMENT_OBJS); do echo "$(srcdir)/$$i" | sed 's/\.o/\.cc/'; done) | $(CLICK_BUILDTOOL) findelem -r userlevel -S > ac_elements.conf
ac_elements.cc: ac_elements.conf $(CLICK_BUILDTOOL)
	$(CLICK_BUILDTOOL) elem2export -S < ac_elements.conf > ac_elements.cc
	@rm -f ac_elements.d

install: ipsumdump ipaggcreate
	$(mkinstalldirs) $(bindir)
	$(INSTALL) ipsumdump $(bindir)/ipsumdump
	$(INSTALL) ipaggcreate $(bindir)/ipaggcreate

always:
	@:

#!gmake
DEPFILES := $(wildcard *.d)
ifneq ($(DEPFILES),)
include $(DEPFILES)
endif
#!end gmake

clean:
	-rm -f *.d *.o sd_elements.mk sd_elements.cc sd_elements.conf \
	ac_elements.mk ac_elements.cc ac_elements.conf ipsumdump
distclean: clean
	-rm -f Makefile

.PHONY: all always clean distclean elemlist
